{
    "sourceType": "module",
    "sourceMaps": false,
    "presets": [
        ["@babel/preset-typescript", {
            "isTSX": true,
            "allExtensions": true,
            "disallowAmbiguousJSXLike": false
        }],
        "@babel/preset-react",
        ["@babel/preset-env", {
            "useBuiltIns": "usage",
            "corejs": "3.6.5"
        }]
    ],
    "plugins": ["babel-plugin-transform-import-meta"]
}